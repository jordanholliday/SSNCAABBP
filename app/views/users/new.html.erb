<h2>Sign Up</h2>
<form action="<%= users_url %>" method="POST">
  <%= auth_token %>
  <p>
    <label for="name">Your Name</label>
    <input type="text" name="user[name]" value="<%= @user.name %>" id="name">
  </p>
  <p>
    <label for="email">Email</label>
    <input type="email" name="user[email]" value="<%= @user.email %>" id="email">
  </p>
  <p>
    <label for="password">Password</label>
    <input type="password" name="user[password]" value="<%= @user.password %>" id="password">
  </p>
  <p>
    <label for="team_name">Team Name</label>
    <input type="text" name="user[team_name]" value="<%= @user.team_name %>" id="team_name">
  </p>
  <p>
    <input type="submit" value="Sign Up" id="sign_up">
  </p>
</form>
